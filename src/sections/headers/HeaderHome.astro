<style>
  header{
    background: url("assets/img/wave-1.svg") var(--primary);
    background-attachment: fixed;
    background-position: bottom;
    background-repeat: no-repeat;
    background-size: contain;
    position: relative;
    width: 100%;
    height: 100vh;
    z-index: -1;
    > article{
      width: 60%;
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      text-align: center;
      user-select: none;
      color: var(--dominant);
      z-index: 40;
      > h2{
        font-size: 6rem;
        font-family: "AlfaSlabOne";
        margin-bottom: 5rem;
      }
      > h1{
        font-size: 2rem;
        font-family: "ZillaSlabHighlight";
      }
    }
    > div#sun{
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      z-index: 20;
      > svg{
        position: absolute;
        top: 5%;
        right: 10%;
      }
    }
    > div#birds{
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      z-index: 30;
      > svg:nth-child(1) {
        position: absolute;
        top: 23%;
        left: 21%;
      }
      > svg:nth-child(2) {
        position: absolute;
        bottom: 17%;
        right: 45%;
      }
      > svg:nth-child(3) {
        position: absolute;
        top: 20%;
        right: 35%;
      }
    }
    > div#mountains{
      position: absolute;
      display: flex;
      justify-content: space-between;
      bottom: 0;
      left: 0;
      width: 100%;
      height: 100%;
      z-index: 50;
      > svg:first-child {
        position: absolute;
        bottom: 0;
        left: 0;
      }
      > svg:last-child {
        position: absolute;
        bottom: 0;
        right: 0;
      }
    }
  }
</style>
<script>
  const text: HTMLElement | null = document.querySelector('header > article')
  const sun: HTMLElement | null = document.querySelector('header > div#sun')
  const birds: HTMLElement | null = document.querySelector('header > div#birds')

  window.addEventListener('scroll', () => {
    let value = window.scrollY;
    
    if (text?.style) text.style.marginTop = value * 0.5 + 'px';
    if (sun?.style) sun.style.marginTop = value * 0.8 + 'px';
    if (birds?.style) birds.style.marginTop = value * 0.5 + 'px';
  })
</script>